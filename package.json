{
  "name": "canaria-web",
  "version": "0.0.0",
  "private": true,
  "description": "canariaのwebアプリケーションです。",
  "scripts": {
    "prestart": "npm run release",
    "start": "babel-node ./src/js/server.js",
    "build:css": "postcss ./src/css/app.css -o ./dist/bundle.css -c ./tasks/postcss",
    "build:js": "webpack",
    "build": "npm-run-all build:*",
    "release:css": "postcss ./src/css/app.css -o ./dist/bundle.css -c ./tasks/postcss --no-map",
    "release:js": "webpack --env release",
    "release": "npm-run-all release:*",
    "watch:js": "webpack -w",
    "watch:css": "postcss ./src/css/app.css -o ./dist/bundle.css -c ./tasks/postcss -m -w",
    "dev:watch": "npm-run-all -p watch:*",
    "dev:server": "gulp dev-server",
    "dev": "npm-run-all -p dev:*",
    "lint": "eslint ./src/js"
  },
  "author": "nabeliwo",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-cookie": "^1.0.5",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-logger": "^3.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "browser-sync": "^2.18.8",
    "csswring": "^5.1.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^3.0.0",
    "postcss-custom-properties": "^5.0.2",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-sorting": "^2.0.1",
    "require-dir": "^0.3.1",
    "webpack": "^2.3.2",
    "webpack-merge": "^4.1.0"
  }
}
