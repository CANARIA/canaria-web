{
  "name": "canaria-web",
  "version": "0.0.0",
  "private": true,
  "description": "canariaのwebアプリケーションです。",
  "scripts": {
    "build:assets:html": "gulp copy:html",
    "build:assets:css": "gulp postcss",
    "build:assets": "npm-run-all --parallel build:assets:*",
    "build:tsc": "tsc -p ./",
    "build:bundle": "webpack --config webpack.config.dev.js",
    "build": "npm-run-all build:assets build:tsc build:bundle",
    "release:assets:html": "gulp copy:html:aot",
    "release:assets:css": "gulp postcss:aot",
    "release:assets:ts": "gulp copy:ts:aot",
    "release:assets": "npm-run-all --parallel release:assets:*",
    "release:ngc": "ngc -p tsconfig.aot.json",
    "release:tsc": "tsc -p ./tsconfig.aot.json",
    "release:bundle": "webpack --config webpack.config.prod.js",
    "release": "npm-run-all release:assets release:ngc release:tsc release:bundle",
    "watch:assets": "gulp watch:assets",
    "watch:ts": "tsc -p ./ -w",
    "watch:bundle": "webpack --config webpack.config.dev.js --watch",
    "watch": "npm-run-all -p watch:*",
    "prewatch": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.4",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.3.0",
    "npm-run-all": "^4.0.1",
    "raw-loader": "^0.5.1",
    "require-dir": "^0.3.1",
    "typescript": "^2.1.5",
    "webpack": "^2.2.0"
  }
}
