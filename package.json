{
  "name": "canaria-web",
  "version": "0.0.0",
  "private": true,
  "description": "canariaのwebアプリケーションです。",
  "scripts": {
    "prestart": "npm run release",
    "start": "babel-node ./src/js/server.js",
    "build:css": "postcss -c ./tasks/postcss/config.json",
    "build:js": "webpack",
    "build": "npm-run-all build:*",
    "release:css": "postcss -c ./tasks/postcss/config.json",
    "release:js": "webpack --env release",
    "release": "npm-run-all release:*",
    "watch:js": "webpack -w",
    "watch:css": "postcss -c ./tasks/postcss/config.json -m -w",
    "watch": "npm-run-all -p watch:*",
    "dev:server": "gulp dev-server",
    "dev": "npm-run-all -p watch dev:server",
    "lint": "eslint ./src/js"
  },
  "author": "nabeliwo",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "node-uuid": "^1.4.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-logger": "^2.8.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.18.6",
    "csswring": "^5.1.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^2.6.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-sorting": "^2.0.1",
    "require-dir": "^0.3.1",
    "webpack": "^2.2.0",
    "webpack-merge": "^2.4.0"
  }
}
