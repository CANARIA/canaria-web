{
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "expect": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
