{
  "extends": "airbnb",
  "rules": {
    "no-shadow": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "camelcase": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "expect": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
